<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ListView
        android:id="@+id/search_ranking_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </ListView>
    
    <FrameLayout
        android:id="@+id/search_cart_container"
        android:layout_width="match_parent"
        android:layout_height="@dimen/cart_height"
        android:layout_gravity="bottom"
        android:visibility="gone" >
    </FrameLayout>

    <TextView
        android:id="@+id/search_cart_above_number_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_marginBottom="400dp"
        android:layout_marginRight="25dp"
        android:text="1" />

    <ImageView
        android:id="@+id/search_cart_strap_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_marginRight="20dp"
        android:src="@drawable/ic_launcher" />

    <TextView
        android:id="@+id/search_cart_number_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_marginBottom="30dp"
        android:layout_marginRight="25dp"
        android:text="1" />

</FrameLayout>