<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ExpandableListView
        android:id="@+id/agreement_terms_list"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:scrollbars="none"
        android:listSelector="@drawable/null_image"
        android:groupIndicator="@drawable/agreement_group_indicator" >

    </ExpandableListView>

    <Button
        android:id="@+id/agreement_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/agreement_btn_selector"
        android:text="동의하고 터치아웃 시작하기" />

</LinearLayout>
