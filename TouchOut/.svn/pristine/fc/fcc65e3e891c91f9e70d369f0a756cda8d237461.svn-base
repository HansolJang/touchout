<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="360dp"
    android:layout_height="640dp"
    android:orientation="vertical" >

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:text="X" />

   <LinearLayout
        android:layout_width="360dp"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/dialog_stamp_store_name_txt"
            android:layout_width="360dp"
            android:layout_height="50dp"
            android:gravity="center"
            android:paddingLeft="20dp"
            android:text="TextView"
            android:textSize="20sp" />

        <GridView
            android:id="@+id/dialog_stamp_grid_view"
            android:layout_width="360dp"
            android:layout_height="120dp"
            android:layout_gravity="center"
            android:numColumns="5" >
        </GridView>

        <TextView
            android:id="@+id/stamp_reward_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="TextView" />

        <TextView
            android:id="@+id/stamp_desc_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="5dp"
            android:text=" * 본 쿠폰은 발급일로부터 1년간 유효하며. 사용기간외에 이용할 수 없음을 명시합니다."
            android:textColor="#777777"
            android:textSize="12sp" />
    </LinearLayout>

   <TextView
       android:id="@+id/terms_child_txt"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_weight="1"
       android:text="TextView"
       android:visibility="invisible" />

    <Button
        android:id="@+id/dialog_stamp_exit_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="닫기" />

</LinearLayout>
