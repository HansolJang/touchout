<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.jroad.touchout.activity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="21" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:name="kr.jroad.touchout.extra.MyApplication">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" android:theme="@style/AppTheme" android:windowSoftInputMode="adjustPan">
        </activity>
        <activity 
            android:name="GreetingActivity"
            android:theme="@style/NoTitleTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="StoreMenuActivity"></activity>
        <activity android:name="ExtraMyWalletActivity"></activity>
        <activity android:name="PaymentActivity"></activity>
        <activity android:name="StoreInfoActivity"></activity>
        <activity android:name="PaymentFinishActivity"></activity>
        <activity android:name="NoticeActivity"></activity>
        <activity android:name="AppInfoActivity"></activity>
        <activity android:name="AccountInfoAcitivity"></activity>
        <activity android:name="WriteReviewActivity"></activity>
        <activity android:name="MapActivity"></activity>
    </application>

</manifest>
