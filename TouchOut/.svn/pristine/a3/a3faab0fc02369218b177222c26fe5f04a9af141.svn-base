<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <ListView
            android:id="@+id/search_result_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@null"
            android:listSelector="@drawable/null_image" >

        </ListView>
    </LinearLayout>

    <FrameLayout
        android:id="@+id/search_result_cart_container"
        android:layout_width="match_parent"
        android:layout_height="@dimen/cart_height"
        android:layout_gravity="bottom"
        android:visibility="gone" >
    </FrameLayout>


    <ImageView
        android:id="@+id/search_result_cart_strap_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="-18dp"
        android:src="@drawable/ic_launcher" />

    <TextView
        android:id="@+id/search_result_cart_number_view"
        android:layout_marginBottom="33dp"
        android:layout_marginRight="23dp"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:background="@drawable/cart_speach_balloon"
        android:layout_gravity="bottom|right"
        android:textColor="#fff"
        android:textSize="11sp"
        android:paddingLeft="5dp"
        android:text="1" />

    <kr.jroad.touchout.view.ListEmptyView
        android:id="@+id/search_result_empty_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" >

    </kr.jroad.touchout.view.ListEmptyView>

</FrameLayout>